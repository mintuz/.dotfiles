{
  "name": "nuclide-move-pane",
  "repository": "https://github.com/facebook/nuclide",
  "main": "./lib/main",
  "version": "0.0.0",
  "description": "Keyboard shortcuts for moving the current tab to a new pane.",
  "nuclide": {
    "packageType": "Atom",
    "testRunner": "apm"
  },
  "activationCommands": {
    "atom-text-editor:not(.mini)": [
      "nuclide-move-pane:move-tab-to-new-pane-up",
      "nuclide-move-pane:move-tab-to-new-pane-down",
      "nuclide-move-pane:move-tab-to-new-pane-left",
      "nuclide-move-pane:move-tab-to-new-pane-right"
    ]
  }
}
